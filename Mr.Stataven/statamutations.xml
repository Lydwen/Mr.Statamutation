<?xml version="1.0" encoding="UTF-8"?>
<statam>
	<mutations>
		<mutation>
			<name>Mutation PlusMinus percent of all</name>
			<processors>
				<processor>fr.polytech.devops.g1.stataspoon.processors.operators.binary.PlusToMinusProcessor</processor>
			</processors>
			<selector name="percent of all">
				<parameters>
					<percent>30.28</percent>
				</parameters>
			</selector>
		</mutation>
		<mutation>
			<name>Mutation comparaisons percent of classes</name>
			<processors>
				<processor>fr.polytech.devops.g1.stataspoon.processors.operators.binary.InfEqToSupEqProcessor</processor>
				<processor>fr.polytech.devops.g1.stataspoon.processors.operators.binary.EqToIneqProcessor</processor>
			</processors>
			<selector name="percent of classes">
				<parameters>
					<percent>37.4</percent>
				</parameters>
			</selector>
		</mutation>
		
		<mutation>
			<name>Mutation And To Or all</name>
			<processors>
				<processor>fr.polytech.devops.g1.stataspoon.processors.operators.binary.AndToOrProcessor</processor>
			</processors>
		</mutation>
		
		<mutation>
			<name>Mutation Eq to Ineq random</name>
			<processors>
				<processor>fr.polytech.devops.g1.stataspoon.processors.operators.binary.EqToIneqProcessor</processor>
			</processors>
			<selector name="random">
				<parameters>
					<percent>37.4</percent>
				</parameters>
			</selector>
		</mutation>
	</mutations>
</statam>
